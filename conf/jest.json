{
    "moduleFileExtensions": [
        "js"
    ],
    "testMatch": [
        "**/*.test.js"
    ],
    "coverageDirectory": "doc/jest",
    "coverageReporters": [
        "lcov"
    ],
    "collectCoverageFrom": [
        "**/*.js",
        "!**/*.test.js",
        "!index.js",
        "!utils.js"
    ]
}
